{
  "name": "splokify",
  "private": true,
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build:development": "webpack --mode development",
    "build:development:watch": "yarn run build:development --watch",
    "start": "run-p build:development:watch start:dev:watch",
    "start:dev": "electron dist/index.js",
    "start:dev:watch": "nodemon --watch \"dist/*\" --exec \"yarn run start:dev\""
  },
  "devDependencies": {
    "@blue-tomato/eslint-config": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "electron": "^8.3.0",
    "electron-builder": "^22.6.1",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "build": {
    "appId": "splokify",
    "productName": "Splokify",
    "directories": {
      "output": "app"
    }
  }
}
